{
    "name": "megacubo-1460",
    "main": "index.html",
    "chromium-args": "--disable-transparency --ignore-certificate-errors --remote-debugging-port=9000     --disable-gpu --force-cpu-draw",
    "description": "A simple and intuitive IPTV player.",
    "version": "14.6.0",
    "inject_js_end": "assets/js/helper.js",
    "bugs": "https://www.facebook.com/messages/t/MegacuboTV",
    "window": {
        "title": "Megacubo",
        "frame": false,
        "icon": "default_icon.png",
        "show": true,
        "width": 640,
        "height": 360
    },
    "dependencies": {
        "async": "~2.6.0",
        "btdb-search": "~1.1.0",
        "chromecasts": "~1.9.0",
        "drag-drop": "~2.13.2",
        "fluent-ffmpeg": "~2.1.2",
        "m3u8-parser": "~4.0.0",
        "miniget": "~1.1.0",
        "mkdirp": "~0.3.5",
        "nw-custom-frame": "~0.9.1",
        "peerflix": "~0.37.0",
        "request": "~2.83.0",
        "ytdl-core": "~0.20.0",
        "ytsr": "~0.1.7"
    },
    "webkit": {
        "page-cache": true
    },
    "permissions": [
        "<all_urls>",
        "alwaysOnTopWindows",
        "http://*/",
        "https://*/"
    ]
}