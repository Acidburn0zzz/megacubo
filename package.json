{
    "name": "megacubo",
    "main": "index.html",
    "chromium-args": "--disable-transparency  --ignore-certificate-errors --remote-debugging-port=9000            --disable-gpu-blacklist",
    "node": true,
    "description": "A quite simple IPTV client.",
    "version": "14.0.6",
    "inject_js_end": "assets/js/helper.js",
    "window": {
        "title": "Megacubo",
        "frame": false,
        "icon": "default_icon.png",
        "show": false
    },
    "dependencies": {
        "peerflix": "~0.37.0",
        "fluent-ffmpeg": "~2.1.2",
        "fetch": "~0.3.5",
        "mkdirp": "~0.3.5",
        "m3u8-parser": "~4.0.0",
        "miniget": "~1.1.0",
        "async": "~2.6.0",
        "drag-drop": "~2.13.2",
        "nw-custom-frame": "~0.9.1",
        "request": "~2.83.0"
    },
    "webkit": {
        "page-cache": false
    },
    "permissions": [
        "alwaysOnTopWindows",
        "http://*/",
        "https://*/"
    ]
}