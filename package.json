{
    "bugs": "https://www.facebook.com/messages/t/MegacuboTV",
    "chromium-args": "--ignore-urlfetcher-cert-requests --tls13-variant=disabled --disable-web-security --allow-hidden-media-playback --disable-features=NetworkPrediction --disable-background-timer-throttling --proxy-auto-detect --no-zygote --no-sandbox --enable-gpu-async-worker-context --password-store=basic --force-device-scale-factor=1 --process-per-tab --enable-smooth-scrolling --disable-crash-handler=true --disable-breakpad --no-experiments --no-pings --disable-transparency --remote-debugging-port=9000",
    "dependencies": {
        "@codemotion/color-thief": "~2.0.5",
        "@fortawesome/fontawesome-free": "~5.8.1",
        "async": "~2.6.0",
        "cached-request": "github:efoxbr/cached-request",
        "chromecasts": "~1.9.0",
        "dnscache": "^1.0.2",
        "drag-drop": "~4.2.0",
        "fluent-ffmpeg": "~2.1.2",
        "fontfaceobserver": "~2.1.0",
        "m3u8-parser": "~4.3.0",
        "md5": "^2.2.1",
        "moment": "~2.24.0",
        "node-ipc": "~9.1.1",
        "nw-custom-frame": "github:efoxbr/nw-custom-frame",
        "p2p-media-loader-core": "~0.5.0",
        "peerflix": "~0.39.0",
        "pornsearch": "~2.4.3",
        "ps-node": "~0.1.6",
        "request": "~2.88.0",
        "subtitler": "~2.6.0",
        "tar": "~4.4.8",
        "ytdl-core": "~0.29.1",
        "ytsr": "~0.1.8"
    },
    "description": "A simple and intuitive IPTV player.",
    "inject_js_end": "assets/js/utils/helper.js",
    "js-flags": "--max_old_space_size=2048",
    "main": "app.html",
    "name": "megacubo",
    "permissions": [
        "<all_urls>",
        "alwaysOnTopWindows",
        "http://*/",
        "https://*/"
    ],
    "version": "15.4.2",
    "webkit": {
        "page-cache": true
    },
    "window": {
        "icon": "default_icon.png",
        "position": "center",
        "title": "Megacubo",
        "frame": false,
        "height": 562,
        "width": 1000,
        "show": true
    }
}
